{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ecrallowfordevops",
            "Effect": "Allow",
            "Action": [
                "ecr:BatchCheckLayerAvailability",
                "ecr:BatchGetImage",
                "ecr:BatchGetRepositoryScanningConfiguration",
                "ecr:CreateRepository",
                "ecr:GetDownloadUrlForLayer",
                "ecr:GetLifecyclePolicy",
                "ecr:GetLifecyclePolicyPreview",
                "ecr:GetRegistryPolicy",
                "ecr:GetRegistryScanningConfiguration",
                "ecr:GetRepositoryPolicy",
                "ecr:PutImage",
                "ecr:PutImageTagMutability",
                "ecr:Complete*",
                "ecr:Describe*",
                "ecr:Initiate*",
                "ecr:List*",
                "ecr:Start*",
                "ecr:Tag*",
                "ecr:Upload*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "s3allowfordevops",
            "Effect": "Allow",
            "Action": [
                "s3:CreateAccessPoint",
                "s3:CreateAccessPointForObjectLambda",
                "s3:CreateBucket",
                "s3:CreateJob",
                "s3:CreateMultiRegionAccessPoint",
                "s3:CreateStorageLensGroup",
                "s3:PutAccessPointConfigurationForObjectLambda",
                "s3:PutAnalyticsConfiguration",
                "s3:PutBucketOwnershipControls",
                "s3:PutBucketTagging",
                "s3:PutBucketVersioning",
                "s3:PutBucketWebsite",
                "s3:PutJobTagging",
                "s3:PutLifecycleConfiguration",
                "s3:PutMetricsConfiguration",
                "s3:PutObject",
                "s3:PutObjectTagging",
                "s3:PutObjectVersionTagging",
                "s3:PutReplicationConfiguration",
                "s3:PutStorageLensConfiguration",
                "s3:PutStorageLensConfigurationTagging",
                "s3:ReplicateObject",
                "s3:ReplicateTags",
                "s3:UpdateJobPriority",
                "s3:UpdateJobStatus",
                "s3:UpdateStorageLensGroup",
                "s3:Describe*",
                "s3:Get*",
                "s3:Initiate*",
                "s3:List*",
                "s3:Object*",
                "s3:Restore*",
                "s3:Tag*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "rdsallowfordevops",
            "Effect": "Allow",
            "Action": [
                "rds:Add*",
                "rds:Apply*",
                "rds:Authorzie*",
                "rds:Backtrack*",
                "rds:Cancel*",
                "rds:Copy*",
                "rds:Create*",
                "rds:Delete*",
                "rds:Deregister*",
                "rds:Describe*",
                "rds:Download*",
                "rds:Failover*",
                "rds:List*",
                "rds:Modify*",
                "rds:Promote*",
                "rds:Purchase*",
                "rds:Reboot*",
                "rds:Register*",
                "rds:Remove*",
                "rds:Reset*",
                "rds:Restore*",
                "rds:Revoke*",
                "rds:Start*",
                "rds:Stop*",
                "rds:Switch*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "elasticacheallowfordevops",
            "Effect": "Allow",
            "Action": [
                "elasticache:Add*",
                "elasticache:Copy*",
                "elasticache:Create*",
                "elasticache:Describe*",
                "elasticache:Export*",
                "elasticache:List*"
            ],
            "Resource": "*"
        }
    ]
}